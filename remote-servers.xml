<application>
  <component name="RemoteServers">
    <remote-server name="StockBlocks" type="docker">
      <configuration email="">
        <option name="apiUrl" value="http://192.168.99.100:2376" />
        <option name="certificatesPath" value="$USER_HOME$/.docker/machine/machines/stockblocks/stockblocks" />
      </configuration>
    </remote-server>
    <remote-server name="Docker" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="$USER_HOME$/Projects/StockBlocks" />
              <option name="virtualMachinePath" value="/app" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
  </component>
</application>